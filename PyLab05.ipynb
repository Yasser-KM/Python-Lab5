        "def dec(n):\n",
        "    decList = list(range(1, n + 1))\n",
        "    for i in reversed(decList):\n",
        "        for y in reversed(decList[:i]):\n",
        "            print(y)\n",
        "        print()\n",
        "n = 7\n",
        "dec(n)"
